    "babel-preset-es2015": "^6.3.13",
    "exports-loader": "^0.6.2",
    "gulp": "^3.9.0",
    "gulp-jsdoc3": "latest",
    "json-loader": "^0.5.4",
    "merge-stream": "^1.0.0",
    "strip-loader": "^0.1.2",
