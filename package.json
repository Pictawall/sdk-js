{
  "name": "pictawall-sdk-javascript",
  "version": "1.0.0",
  "description": "JavaScript SDK for the Pictawall API",
  "keywords": [
    "pictawall",
    "sdk"
  ],
  "author": "Jean-Christophe Cuvelier <jcc@pictawall.com>",
  "dependencies": {
    "ampersand-collection": "^1.6.1",
    "ampersand-model": "^5.0.3"
  },
  "scripts": {
    "dist": "NODE_ENV=production gulp dist",
    "dist-dev": "NODE_ENV=development gulp dist"
